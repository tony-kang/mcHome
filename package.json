		"@sveltejs/kit": "^2.39.1",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/postcss": "^4.1.13",
		"@tailwindcss/typography": "^0.5.16",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"postcss": "^8.5.6",
